<script>
    $(document).ready(function () {
        loaddata();
    });
    function loaddata() {
        var param = {
            "MaThe": '@ViewBag.accountID'
        }
        $.ajax({
            url: '/TheTai/LoadKetThuc',
            type: 'POST',
            dataType: 'html',
            data:param,
            success: function (data, status, xhr) {
                $("#tbody").html(data);
            },
            error: function (xhr, status, error) {
                alert(status);
            }
        });
    }
    function slChange() {
        loaddata();
    }
</script>